# Copy this file to .env and replace the placeholder values with your actual API keys

# Google API credentials
GOOGLE_APPLICATION_CREDENTIALS=your-service-account-file.json
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_CSE_ID=your-google-cse-id-here

# Microsoft API credentials
MICROSOFT_CLIENT_ID=your-microsoft-client-id-here
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret-here
MICROSOFT_TENANT_ID=your-microsoft-tenant-id-here
MICROSOFT_SUBSCRIPTION_KEY=your-microsoft-subscription-key-here

# Search API credentials
SERPER_API_KEY=your-serper-api-key-here
BRAVE_API_KEY=your-brave-api-key-here
BING_API_KEY=your-bing-api-key-here

# LLM API credentials
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# Other API credentials
WOLFRAMALPHA_APPID=your-wolframalpha-app-id-here
NEWSAPI_KEY=your-newsapi-key-here

# LLM Configuration
JARVIS_LLM_PROVIDER=auto
HUGGINGFACE_URL=transformers
HUGGINGFACE_MODEL=finetune/results_peft
